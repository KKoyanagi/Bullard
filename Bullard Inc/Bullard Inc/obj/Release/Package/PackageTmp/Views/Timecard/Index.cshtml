@{
    Layout = "~/Views/Shared/FieldWorkerFrame.cshtml";

    string status = "OPEN";

    string startDate = "12/6/17";
    string endDate = "12/13/17";
}
<h2 align="center"><b>TIMECARD</b></h2>

<div class="container">
    <div class="row">
        <div class="col"></div>
        <div class="col">
            <nav>
                <ul class="pagination pagination-lg pg-bluegrey">
                    <li class="page-item">
                        <a class="page-link" href="#" aria-label="Previous">
                            <span aria-hidden="true">&laquo;</span>
                            <span class="sr-only">Previous</span>
                        </a>
                    </li>
                    <li class="page-item active">
                        <a class="page-link" href="history">
                            @startDate - @endDate
                        </a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="#" aria-label="Next">
                            <span aria-hidden="true">&raquo;</span>
                            <span class="sr-only">Next</span>
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
        <div class="col"></div>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        <p>Status: <span style="color: red"><b>@status</b></span></p>
    </div>
</div>
<!-- need to pass timesheet id and day id-->
<a href="/timecard/empjobview/1" class="btn btn-default btn-lg btn-block">MONDAY</a>
<a href="/timecard/empjobview/2" class="btn btn-default btn-lg btn-block">TUESDAY</a>
<a href="/timecard/empjobview/3" class="btn btn-default btn-lg btn-block">WEDNESDAY</a>
<a href="/timecard/empjobview/4" class="btn btn-default btn-lg btn-block">THURSDAY</a>
<a href="/timecard/empjobview/5" class="btn btn-default btn-lg btn-block">FRIDAY</a>
<a href="/timecard/empjobview/6" class="btn btn-default btn-lg btn-block">SATURDAY</a>
<a href="/timecard/empjobview/7" class="btn btn-default btn-lg btn-block">SUNDAY</a>
<br />

<div align="right">
    <a class="btn btn-default" href="#">Submit</a>
</div>
